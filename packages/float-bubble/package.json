{
  "name": "@viarotel-org/float-bubble",
  "type": "module",
  "version": "0.1.0",
  "description": "float-bubble vue component",
  "author": "viarotel <viarotel@qq.com>",
  "homepage": "https://github.com/viarotel-org/packages#readme",
  "license": "ISC",
  "main": "dist/float-bubble.umd.cjs",
  "module": "dist/float-bubble.js",
  "exports": {
    "import": "./dist/vue3/float-bubble.js",
    "require": "./dist/vue3/float-bubble.umd.js",
    "style": "./dist/vue3/style.css",
    "vue2": "./dist/vue2/float-bubble.js",
    "vue3": "./dist/vue3/float-bubble.js",
    "core": "./src/libs/float-bubble/index.js"
  },
  "files": [
    "./dist/*",
    "./src/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viarotel-org/packages.git"
  },
  "bugs": {
    "url": "https://github.com/viarotel-org/packages/issues"
  },
  "scripts": {
    "dev": "pnpm switch:vue3 && vite --config vite.config.vue3.js",
    "dev:vue2": "pnpm switch:vue2 && vite --config vite.config.vue2.js",
    "build": "pnpm build:vue2 && pnpm build:vue3",
    "build:vue2": "pnpm switch:vue2 && vite --config vite.config.vue2.js build",
    "build:vue3": "pnpm switch:vue3 && vite --config vite.config.vue3.js build",
    "switch:vue3": "vue-demi-switch 3",
    "switch:vue2": "vue-demi-switch 2 vue2",
    "preview": "vite preview",
    "lint": "pnpm run lint:js",
    "lint:js": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .eslintignore --fix"
  },
  "dependencies": {
    "@vue/composition-api": "^1.7.1",
    "lodash-es": "^4.17.21",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@unocss/reset": "^0.53.5",
    "@unocss/transformer-directives": "^0.53.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue2": "^2.2.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.15.1",
    "postcss-nested": "^6.0.1",
    "postcss-remove-inline-comments": "^0.0.2",
    "postcss-scss": "^4.0.6",
    "unocss": "^0.53.5",
    "vite": "^4.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.1",
    "vue-template-compiler": "^2.7.14",
    "vue2": "npm:vue@^2.7.14"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
